include tests
#we put the JRE and ASPECTJ into the image file system to avoid bootstrap issues
imagefile  ${aspectj.home}/lib /aspectj/lib

imagefile ${java.home}/../lib/tools.jar /${java.version}/lib/tools.jar
imagefile ${java.home}/lib/rt.jar /$${java.version}/jre/lib/rt.jar
imagefile ${java.home}/lib/jce.jar /$${java.version}/jre/lib/jce.jar
imagefile ${java.home}/lib/jsse.jar /$${java.version}/jre/lib/jsse.jar
imagefile ${java.home}/lib/resources.jar /$${java.version}/jre/lib/resources.jar
imagefile ${java.home}/lib/charsets.jar /$${java.version}/jre/lib/charsets.jar
imagefile ${java.home}/lib/management-agent.jar /$${java.version}/jre/li/management-agent.jar
imagefile ${java.home}/lib/calendars.properties /$${java.version}/jre/lib/calendars.properties
imagefile ${java.home}/lib/content-types.properties /$${java.version}/jre/lib/content-types.properties
imagefile ${java.home}/lib/flavormap.properties /$${java.version}/jre/lib/flavormap.properties
imagefile ${java.home}/lib/logging.properties /$${java.version}/jre/lib/logging.properties
imagefile ${java.home}/lib/net.properties /$${java.version}/jre/lib/net.properties
imagefile ${java.home}/lib/psfontj2d.properties /$${java.version}/jre/lib/psfontj2d.properties 
imagefile ${java.home}/lib/sound.properties /$${java.version}/jre/lib/sound.properties
imagefile ${java.home}/lib/security /$${java.version}/jre/lib/security
imagefile ${java.home}/lib/zi /$${java.version}/jre/lib/zi
