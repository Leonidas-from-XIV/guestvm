# Basic imagespec file for running io/net tests
# Includes the classes for all the tests in the image file system
include imagespec.io
include imagespec.net
imagefsprefix /guestvm/image
imagefile ../GuestVM/bin/test /GuestVM/bin/test
imagefile ../../maxine/VM/bin/test/output /maxine/VM/bin/test/output
imagefile ../../maxine/VM/bin/util /maxine/VM/bin/util
package test.jvmni
forcemethod com.sun.guestvm.test.VmThreadTestHelper.*
forcemethod com.sun.guestvm.test.VMTestHelper.*
