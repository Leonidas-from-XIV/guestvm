forcemethod com.sun.guestvm.spinlock.tas.c.CTTASSpinLock.*
forcemethod com.sun.guestvm.spinlock.tas.c.CTASTSpinLock.*
forcemethod com.sun.guestvm.spinlock.tas.p.PTASSpinLock.*
forcemethod com.sun.guestvm.spinlock.tas.p.PTTASSpinLock.*
forcemethod com.sun.guestvm.spinlock.tas.p.PTASTSpinLock.*
forcemethod com.sun.guestvm.spinlock.tas.cp.PCTASSpinLock.*
forcemethod com.sun.guestvm.spinlock.tas.cp.PCTTASSpinLock.*
forcemethod com.sun.guestvm.spinlock.tas.cp.PCTASTSpinLock.*
